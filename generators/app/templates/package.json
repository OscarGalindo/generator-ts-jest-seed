  "devDependencies": {
    "@types/node": "10.5.8",
    "cross-env": "5.2.0",
    "jest-extended": "0.8.1",
    "ts-jest": "23.1.3",
    "jest": "23.5.0",
    "ts-node": "7.0.0"
  },
  "dependencies": {
    "typescript": "3.0.1"
